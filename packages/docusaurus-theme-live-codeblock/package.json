  "description": "Docusaurus live code block component.",
  "main": "lib/index.js",
  "types": "src/theme-live-codeblock.d.ts",
  "sideEffects": [
    "lib/theme/Playground/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn build:server && yarn build:client && yarn build:copy && yarn build:format",
