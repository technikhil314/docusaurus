    "@docusaurus/utils-validation": "2.0.0-beta.18",
    "@philpl/buble": "^0.19.7",
    "clsx": "^1.1.1",
    "fs-extra": "^11.1.0",
    "react-live": "2.2.3",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@docusaurus/types": "2.0.0-beta.18",
    "@types/buble": "^0.20.1"
  },
  "peerDependencies": {
    "react": "^16.8.4 || ^17.0.0",
